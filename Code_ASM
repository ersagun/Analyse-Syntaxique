|============ Header ================|
.include beta.uasm
	CMOVE(stack, SP)
BR(start)
|Generation du programme: appel de 1 fontion(s)|
main:
	PUSH(LP)
	PUSH(BP)
	MOVE(SP, BP)
	ALLOCATE(0)
	DEALLOCATE(0)
	POP(BP)
	POP(LP)
	JMP(LP)
|============ DÃ©but programme ================|
start:
	CALL(main)
	HALT()
stack:
